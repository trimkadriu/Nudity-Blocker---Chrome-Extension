{
	"manifest_version": 2,
	"name": "Nudity Blocker",
	"description": "This extension blocks nude images and videos by scanning",
	"version": "0.0.1",
	"browser_action": {
		"default_title": "Nudity Blocker",
		"default_icon": "assets/icon/icon48a.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "assets/icon/icon16a.png",
		"48": "assets/icon/icon48a.png",
		"128": "assets/icon/icon128a.png"
	},
	"background": {
		"scripts": [
			"assets/js/background.js",
			"assets/js/jquery.min.js"
		]
	},
	"permissions": [
		"webNavigation",
		"background",
		"tabs",
		"storage",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],
	"web_accessible_resources": [
		"assets/images/blocked.png"
	]
}